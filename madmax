<newValue><input inputmode="text" placeholder=":" id="fullName" name="fullName" type="text" value="" autocomplete="name" required /> <input placeholder="Numero do celular" class="input-action" id="user.attributes.numeroTelefone" name="user.attributes.numeroTelefone" required /> <input placeholder="Confirma numero celular" class="input-action" id="numeroTelefoneConfirma" name="numeroTelefoneConfirma" required /></newValue>

<newHeaderRegex>false</newHeaderRegex>

